{
  "name": "@instantlyeasy/claude-code-sdk-ts",
  "version": "0.3.3",
  "description": "Unofficial TypeScript port of the official Python Claude Code SDK",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "find-unused": "knip",
    "examples": "tsx examples/menu",
    "guide": "tsx guide"
  },
  "keywords": [
    "claude",
    "claude-code",
    "anthropic",
    "ai",
    "sdk",
    "typescript"
  ],
  "author": "Daniel King & Claude",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "execa": "9.6.0",
    "js-yaml": "^4.1.0",
    "which": "5.0.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "24.9.1",
    "@types/which": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitest/coverage-v8": "4.0.2",
    "blessed": "^0.1.81",
    "cli-highlight": "^2.1.11",
    "eslint": "9.38.0",
    "knip": "^5.66.3",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instantlyeasy/claude-code-sdk-ts.git"
  },
  "bugs": {
    "url": "https://github.com/instantlyeasy/claude-code-sdk-ts/issues"
  },
  "homepage": "https://github.com/instantlyeasy/claude-code-sdk-ts#readme"
}