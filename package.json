{
  "name": "@instantlyeasy/claude-code-sdk-ts",
  "version": "0.3.3",
  "description": "Unofficial TypeScript port of the official Python Claude Code SDK",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "examples",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsgo --noEmit",
    "lint": "biome check --write",
    "lint:fix": "biome check --write",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "anthropic",
    "ai",
    "sdk",
    "typescript"
  ],
  "author": "Daniel King & Claude",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "execa": "^9.6.1",
    "js-yaml": "^4.1.1",
    "which": "^6.0.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.3.0",
    "@types/which": "^3.0.4",
    "@biomejs/biome": "^2.4.4",
    "@vitest/coverage-v8": "^4.0.18",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "@typescript/native-preview": "^7.0.0-dev.20260223.1",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instantlyeasy/claude-code-sdk-ts.git"
  },
  "bugs": {
    "url": "https://github.com/instantlyeasy/claude-code-sdk-ts/issues"
  },
  "homepage": "https://github.com/instantlyeasy/claude-code-sdk-ts#readme"
}
